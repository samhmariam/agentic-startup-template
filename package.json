{
  "name": "agentic-startup-template",
  "version": "0.1.0",
  "description": "The OS for your Agentic Pod â€” five-role Knowledge Flywheel built on Vercel AI SDK + ChromaDB",
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test:unit": "vitest run tests/unit",
    "test:agentic": "vitest run tests/agentic",
    "test": "vitest run",
    "evals": "tsx scripts/evals/run-evals.ts",
    "seed": "tsx scripts/seed-context.ts",
    "flywheel": "tsx src/core/flywheel.ts",
    "audit:ci": "tsx scripts/run-agentic-audit.ts",
    "dev": "tsx --watch src/core/flywheel.ts"
  },
  "dependencies": {
    "ai": "^4.3.0",
